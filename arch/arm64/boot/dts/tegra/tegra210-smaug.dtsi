		reg = <0x1c>;
		interrupt-parent = <&gpio>;
		interrupts = <TEGRA_GPIO(A, 6) IRQ_TYPE_LEVEL_LOW>;
		regulator-name = "PPVAR_GPU";
		regulator-min-microvolt = <840000>;
		regulator-max-microvolt = <1150000>;
		regulator-ramp-delay = <12500>;
